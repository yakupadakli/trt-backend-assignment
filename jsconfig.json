{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES2020",
    "jsx": "react",
    "allowJs": true,
    "strict": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "checkJs": false
  },
  "exclude": ["node_modules", "**/node_modules/*"]
}
